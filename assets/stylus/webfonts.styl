font-url(file)
  return '../media/fonts/' + file

webfont(family, file, weight = 'normal')
  @font-face
    font-family family
    src url(font-url(file + '.woff')) format('woff')
    font-weight weight
    font-style normal
    font-stretch normal

@import url('//fonts.googleapis.com/css?family=Space+Mono')
@import url('https://fonts.googleapis.com/icon?family=Material+Icons')
webfont("Regular", "Regular-Semibold", "normal")
webfont("AtlasGroteskRegular", "AtlasGrotesk-Regular-Web", "normal")

